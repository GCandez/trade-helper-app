{
  "files.exclude": {
    ".parcel-cache": true,
    ".scannerwork": true,
    "dist": true,
    "node_modules": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.suggest.insertMode": "replace"
  },
  "files.eol": "\n",
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
